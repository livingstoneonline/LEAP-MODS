  <mods:name type='personal'>
    <mods:namePart>Buchanan, Andrew, 1798-1882</mods:namePart>
    <mods:role>
      <mods:roleTerm type='text'>addressee</mods:roleTerm>
    </mods:role>
    <mods:description>Professor of the Institutes of Medicine at Anderson's College, Glasgow. Was one of Livingstone's Trustees.</mods:description>
  </mods:name>
  <mods:name type='personal'>
    <mods:namePart>Young, James, 1811-1883</mods:namePart>
    <mods:role>
      <mods:roleTerm type='text'>addressee</mods:roleTerm>
    </mods:role>
    <mods:description>Scottish industrialist who made a fortune distilling paraffin from shale. Met Livingstone at Anderson's College. Was one of Livingstone's three Trustees.</mods:description>
  </mods:name>
  <mods:name type='personal'>
    <mods:namePart>Hannan, James</mods:namePart>
    <mods:role>
      <mods:roleTerm type='text'>addressee</mods:roleTerm>
    </mods:role>
    <mods:description>Civic officer. Associated with Henry Monteith &amp; Co, turkey red dyers, Glasgow. Bailie of Glasgow. Was one of Livingstone's three trustees.</mods:description>
  </mods:name>
